# MINING ENCHANTED ITEM CRAFTING

# COBBLESTONE

# normal
execute at @e[type=minecraft:marker,name=dropcrafter] unless data block ~ ~ ~ Items[{Slot:0b}] if data block ~ ~ ~ Items[{Slot:1b,id:"minecraft:cobblestone",Count:32b}] unless data block ~ ~ ~ Items[{Slot:2b}] if data block ~ ~ ~ Items[{Slot:3b,id:"minecraft:cobblestone",Count:32b}] if data block ~ ~ ~ Items[{Slot:4b,id:"minecraft:cobblestone",Count:32b}] if data block ~ ~ ~ Items[{Slot:5b,id:"minecraft:cobblestone",Count:32b}] unless data block ~ ~ ~ Items[{Slot:6b}] if data block ~ ~ ~ Items[{Slot:7b,id:"minecraft:cobblestone",Count:32b}] unless data block ~ ~ ~ Items[{Slot:8b}] run data merge block ~ ~ ~ {Items:[{Slot:4b,id:"minecraft:cobblestone",Count:1b,tag:{display:{Name:'[{"text":"Enchanted Cobblestone","italic":false,"color":"green"}]'},Enchantments:[{id:protection,lvl:1}],HideFlags:1,Enchanted:1b}}]}

