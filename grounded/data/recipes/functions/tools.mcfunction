# Sweet Axe
execute at @e[type=minecraft:marker,name="dropcrafter"] if data block ~ ~ ~ Items[{Slot:0b,id:"minecraft:iron_ingot",Count:1b}] if data block ~ ~ ~ Items[{Slot:1b,id:"minecraft:iron_ingot",Count:1b,tag:{Enchanted:1b}}] if data block ~ ~ ~ Items[{Slot:2b,id:"minecraft:sugar",Count:1b,tag:{Enchanted:1b}}] if data block ~ ~ ~ Items[{Slot:3b,id:"minecraft:iron_ingot",Count:1b}] if data block ~ ~ ~ Items[{Slot:4b,id:"minecraft:stick",Count:1b}] unless data block ~ ~ ~ Items[{Slot:5b}] unless data block ~ ~ ~ Items[{Slot:6b}] if data block ~ ~ ~ Items[{Slot:7b,id:"minecraft:stick",Count:1b}] unless data block ~ ~ ~ Items[{Slot:8b}] run data merge block ~ ~ ~ {Items:[{Slot:4b,id:"minecraft:iron_axe",Count:1b,tag:{Unbreakable:1b,SweetAxe:1b,display:{Name:'[{"text":"Sweet Axe","italic":false}]'}}}]}
# AOTE
execute at @e[type=minecraft:marker,name="dropcrafter"] if data block ~ ~ ~ Items[{Slot:0b,id:"minecraft:popped_chorus_fruit",Count:1b,tag:{Enchanted:1b}}] if data block ~ ~ ~ Items[{Slot:1b,id:"minecraft:dragon_egg",Count:1b}] if data block ~ ~ ~ Items[{Slot:2b,id:"minecraft:popped_chorus_fruit",Count:1b,tag:{Enchanted:1b}}] unless data block ~ ~ ~ Items[{Slot:3b}] if data block ~ ~ ~ Items[{Slot:4b,id:"minecraft:stick",Count:1b}] unless data block ~ ~ ~ Items[{Slot:5b}] unless data block ~ ~ ~ Items[{Slot:6b}] if data block ~ ~ ~ Items[{Slot:7b,id:"minecraft:stick",Count:1b}] unless data block ~ ~ ~ Items[{Slot:8b}] run data merge block ~ ~ ~ {Items:[{Slot:4b,id:"minecraft:carrot_on_a_stick",Count:1b,tag:{aote:1b,display:{Name:'[{"text":"Aspect of the End","italic":false,"color":"light_purple"}]',Lore:['[{"text":"Teleports you up","color":"dark_gray"}]','[{"text":"to 8 blocks forward","italic":true,"color":"dark_gray"}]']}}}]}
